{
    "name": "softlogic-gt/laravel-validation-rules-container",
    "description": "A set of useful Laravel validation rules shipping containers",
    "keywords": [
        "softlogic",
        "laravel-validation-rules-container"
    ],
    "homepage": "https://github.com/softlogic-gt/laravel-validation-rules-container",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Galindo",
            "email": "jgalindo@softlogic.com.gt",
            "homepage": "https://softlogic.com.gt",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "SoftlogicGT\\ValidationRulesContainer\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoftlogicGT\\ValidationRulesContainer\\ValidationRulesContainerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
